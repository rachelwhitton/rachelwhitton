{
  "name": "rachelwhitton/rachelwhitton",
  "description":"Personal WP Site used mostly for testing things for docs",
  "type": "composer",
  "minimum-stability": "dev",
  "config"      : {
    "vendor-dir": "wp-content/vendor"
  },
  "authors": [
    {
      "name": "Rachel",
      "email": "rachel@pantheon.io"
    }
  ],
  "repositories": {
    "petenelson/wp-rest-api-log": {
      "type": "package",
      "package": {
        "version": "dev-master",
        "name": "petenelson/wp-rest-api-log",
        "type": "wordpress-plugin",
        "source": {
          "type": "git",
          "url": "https://github.com/petenelson/wp-rest-api-log",
          "reference": "master"
        }
      }
    },
    "wp-api/oauth1": {
      "type": "package",
      "package": {
        "version": "dev-master",
        "name": "wp-api/oauth1",
        "type": "wordpress-plugin",
        "source": {
          "type": "git",
          "url": "https://github.com/wp-api/oauth1",
          "reference": "master"
        }
      }
    },
    "wpackagist": {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    "packagist": {
      "type": "composer",
      "url": "https://packagist.org/"
    }
  },
  "require-dev": {
    "wpackagist-plugin/debug-bar": "dev-trunk",
    "petenelson/wp-rest-api-log": "*"
  },
  "require": {
    "composer/installers": "^1.0.21",
    "koodimonni/composer-dropin-installer": "*",
    "wpackagist-plugin/wp-cfm": "1.*",
    "wpackagist-plugin/rest-api": "2.0-beta13",
    "wpackagist-plugin/solr-power": "*",
    "wpackagist-plugin/pantheon-hud": "0.1.0",
    "wpackagist-plugin/wp-native-php-sessions": "0.5",
    "wpackagist-plugin/wp-redis": "0.*",
    "wpackagist-plugin/wordpress-seo": "3.3.4",
    "wpackagist-plugin/google-analytics-for-wordpress": "5.*",
    "wpackagist-theme/twentysixteen": "1.*"
  },
  "extra": {
    "installer-paths": {
      "wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "wp-content/themes/{$name}/": ["type:wordpress-theme"]
    },
    "dropin-paths": {
      "wp-content": [
        "package:wpackagist-plugin/wp-redis:object-cache.php"
      ]
    }
  }
}
