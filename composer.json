{
  "name": "rachelwhitton/rachelwhitton",
  "description":"Personal WP Site",
  "type": "composer",
  "minimum-stability": "dev",
  "config"      : {
    "vendor-dir": "wp-content/vendor"
  },
  "authors": [
    {
      "name": "Rachel",
      "email": "rachel@pantheon.io"
    }
  ],
  "repositories": {
    "wpackagist": {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    "packagist": {
      "type": "composer",
      "url": "https://packagist.org/"
    },
    "my-custom-plugin": {
      "type": "package",
      "package": {
        "name": "rachelwhitton/my-custom-plugin",
        "type": "wordpress-plugin",
        "version": "dev-master",
        "source": {
          "type": "git",
          "url": "https://github.com/rachelwhitton/my-custom-plugin.git",
          "reference": "master"
        }
      }
    },
    "drop-in-redis": {
      "type": "package",
      "package": {
        "name": "rachelwhitton/drop-in-redis",
        "type": "drop-in",
        "version": "dev-master",
        "source": {
          "type": "git",
          "url": "https://github.com/rachelwhitton/drop-in-redis.git",
          "reference": "master"
        }
      }
    }
  },
  "require-dev": {
    "wpackagist-plugin/solr-power": "dev-trunk",
    "rachelwhitton/my-custom-plugin": "dev-master",
    "rachelwhitton/drop-in-redis": "dev-master"
  },
  "require": {
    "composer/installers": "^1.0.21",
    "afragen/github-updater": "^4.6",
    "koodimonni/composer-dropin-installer": "*",
    "wpackagist-plugin/wp-cfm": "1.*",
    "wpackagist-plugin/pantheon-hud": "0.1.0",
    "wpackagist-plugin/wp-native-php-sessions": "0.5",
    "wpackagist-plugin/wp-redis": "0.4.0",
    "wpackagist-plugin/wordpress-seo": "3.1.2"
  },
  "extra": {
    "installer-paths": {
      "wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "wp-content/themes/{$name}/": ["type:wordpress-theme"]
    },
    "dropin-paths": {
      "wp-content": [
        "package:wpackagist-plugin/wp-redis:object-cache.php"
      ]
    }
  }
}
